{
  "name": "trigle",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true,
    "SoftDelete": true
  },
  "properties": {
    "title": {
      "type": "string"
    },
    "describe": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "posts": {
      "type": "hasMany",
      "model": "post",
      "foreignKey": "trigleId"
    },
    "customers": {
      "type": "hasMany",
      "model": "customer",
      "foreignKey": "trigleId",
      "through": "joinTrigle"
    }
  },
  "acls": [],
  "methods": {}
}
